{
  "name": "@fit-tracker/server",
  "type": "module",
  "version": "1.0.0",
  "license": "MIT",
  "exports": {
    "./routes": "./src/routes/index.ts",
    "./schema": "./src/db/schema/index.ts"
  },
  "scripts": {
    "build": "tsc && tsc-alias",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:seed": "tsx src/db/seeds/index.ts",
    "db:studio": "drizzle-kit studio",
    "dev": "tsx watch src/index.ts",
    "lint:fix": "pnpm run lint --fix",
    "lint": "eslint .",
    "start": "node -r dotenv/config dist/src/index.js dotenv_config_path=.env.production",
    "test": "cross-env NODE_ENV=test vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@hono/node-server": "^1.12.4",
    "@hono/zod-openapi": "^0.19.8",
    "@libsql/client": "^0.15.8",
    "@scalar/hono-api-reference": "^0.9.2",
    "better-auth": "^1.3.4",
    "dotenv": "^17.0.1",
    "dotenv-expand": "^12.0.2",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.3",
    "drizzle-zod": "^0.8.2",
    "hono": "^4.9.4",
    "hono-pino": "^0.9.1",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "stoker": "1.4.3",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.13.3",
    "@types/node": "^24.0.10",
    "cross-env": "^7.0.3",
    "eslint": "^9.28.0",
    "eslint-plugin-format": "^1.0.1",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.1"
  }
}
