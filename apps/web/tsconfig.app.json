{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "incremental": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "jsx": "react-jsx",
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "baseUrl": "./",
    "paths": {
      "@/web/*": ["./src/*"],
      "@/dtos/auth/*": ["../server/src/modules/auth/dtos/*"],
      "@/dtos/exercises/*": ["../server/src/modules/exercises/dtos/*"],
      "@/dtos/sets/*": ["../server/src/modules/sets/dtos/*"],
      "@/dtos/workouts/*": ["../server/src/modules/workouts/dtos/*"]
    },
    "allowImportingTsExtensions": true,
    "noEmit": true
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"]
}
